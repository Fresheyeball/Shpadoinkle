{
  "name": "Shpadoinkle developer tools",
  "description": "Developer tooling for Shpadoinkle apps",
  "author": "Isaac Shapira fresheyeball@protonmail.com",
  "version": "0.0.0",
  "manifest_version": 2,
  "devtools_page": "main.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["inject.js"]
  }],
  "icons": {
    "128": "icon.png"
  }
}
